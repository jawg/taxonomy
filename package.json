{
  "name": "taxonomy",
  "version": "1.1.0",
  "scripts": {
    "build": "rollup -c",
    "format": "npm run format-html && npm run format-js && npm run format-riot",
    "format-html": "find src demo -name '*.html' -exec js-beautify --config .jsbeautifyrc -f {} --type html -r \\;",
    "format-js": "find src -name '*.js' -exec js-beautify --config .jsbeautifyrc -f {} --type js -r \\;",
    "format-riot": "find src -name '*.riot' -exec js-beautify --config .jsbeautifyrc -f {} --type html -r \\;",
    "watch": "rollup -c -w"
  },
  "description": "Building a map taxonomy chart with style.json",
  "keywords": [
    "Map",
    "Taxonomy",
    "Chart"
  ],
  "author": "Jawg Maps",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "color-operations-ui": "^1.1.0",
    "js-beautify": "^1.13.0",
    "riot": "^5.1.1",
    "rollup": "^2.34.2",
    "rollup-plugin-app-utils": "^1.0.6",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-css-porter": "^1.0.2",
    "rollup-plugin-riot": "^5.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "webfontloader": "^1.6.28"
  },
  "dependencies": {}
}
